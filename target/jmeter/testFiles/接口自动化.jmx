String password=&quot;123456&quot;; 
String method=&quot;user.login&quot;; 
String v=&quot;1.0&quot;; 
String KEY=&quot;A1B2C3D4E5F60708&quot;; 
String a = &quot;{\&quot;userName\&quot;:&apos;&quot;+username+&quot;&apos;,\&quot;password\&quot;:&apos;&quot;+password+&quot;&apos;,\&quot;deviceType\&quot;:\&quot;ios\&quot;,\&quot;deviceToken\&quot;:\&quot;\&quot;,\&quot;imgCode\&quot;:\&quot;\&quot;}&quot;;
